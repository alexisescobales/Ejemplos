<!DOCTYPE html>
<html>
<head>
<title>sort</title>
</head>
<body>
<script>
//ORDENA EL ARRAY ASCENDENTEMENTE
const edades = [3, 7, 2, 26, 8, 18, 20];

edades.sort((a, b) => {
  if(a == b) {
    return 0; 
  }
  if(a < b) {
    return -1;
  }
  return 1;
});

console.log(edades);

</script>
</body>
</html>
